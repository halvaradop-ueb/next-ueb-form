{
  "name": "@ueb/evaluation-system",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Evaluation system monorepo",
  "scripts": {
    "dev": "turbo dev --parallel",
    "dev:api": "turbo dev --filter=api",
    "dev:web": "turbo dev --filter=web",
    "dev:server": "pnpm --filter api dev:server",
    "build": "turbo build --parallel",
    "build:web": "turbo build --filter=web",
    "build:api": "turbo build --filter=api",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "Hernan Alvarado <hernanvid123@gmail.com>",
  "bugs": {
    "url": "https://github.com/halvaradop-ueb/next-ueb-form/issues"
  },
  "homepage": "https://github.com/halvaradop-ueb/next-ueb-form",
  "devDependencies": {
    "@halvaradop/eslint-config": "^1.0.0",
    "@halvaradop/prettier-config": "^1.2.0",
    "@halvaradop/ts-utility-types": "^0.20.0",
    "@halvaradop/tsconfig": "^1.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0"
  }
